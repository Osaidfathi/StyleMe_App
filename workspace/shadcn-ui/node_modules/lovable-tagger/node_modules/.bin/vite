#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/osaid/OneDrive/Desktop/Project/StyleMe_Vercel_Ready_Project/workspace/shadcn-ui/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.17/node_modules/vite/bin/node_modules:/mnt/c/Users/osaid/OneDrive/Desktop/Project/StyleMe_Vercel_Ready_Project/workspace/shadcn-ui/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.17/node_modules/vite/node_modules:/mnt/c/Users/osaid/OneDrive/Desktop/Project/StyleMe_Vercel_Ready_Project/workspace/shadcn-ui/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.17/node_modules:/mnt/c/Users/osaid/OneDrive/Desktop/Project/StyleMe_Vercel_Ready_Project/workspace/shadcn-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/osaid/OneDrive/Desktop/Project/StyleMe_Vercel_Ready_Project/workspace/shadcn-ui/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.17/node_modules/vite/bin/node_modules:/mnt/c/Users/osaid/OneDrive/Desktop/Project/StyleMe_Vercel_Ready_Project/workspace/shadcn-ui/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.17/node_modules/vite/node_modules:/mnt/c/Users/osaid/OneDrive/Desktop/Project/StyleMe_Vercel_Ready_Project/workspace/shadcn-ui/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.17/node_modules:/mnt/c/Users/osaid/OneDrive/Desktop/Project/StyleMe_Vercel_Ready_Project/workspace/shadcn-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite@5.4.19_@types+node@22.15.17/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../vite@5.4.19_@types+node@22.15.17/node_modules/vite/bin/vite.js" "$@"
fi
